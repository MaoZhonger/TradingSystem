{
    "numOfSelections":3,

    "tradingParameters":
    {
        "horizon":20,
        "stopLoss":25,
        "takeProfit":45,
        "adhereToFilter":false,
        "weighting":0.01
    },

    "filter":
    {
        "PE":["<",25],
        "ROE":[">",10]
    },

    "rankFactors":
    {
        "ROE":{"orderBy":"highest","weight":1},
        "PE":{"orderBy":"lowest","weight":1}
    },

    "frequency":
    {
        "schedule":
        {
            "MONDAY":true,
            "TUESDAY":true,
            "WEDNESDAY":true,
            "THURSDAY":true,
            "FRIDAY":true
        },

        "repeatInWeeks":2
    }
}